{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxt/typescript-build",
      "@types/node",
      "@nuxtjs/composition-api/module",
      "@nuxtjs/i18n",
      "nuxt-graphql-request",
      "@nuxt/http",
      "@nuxtjs/sentry",
      "@pinia/nuxt",
      "cookie-universal-nuxt"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
