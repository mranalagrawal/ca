{
  "version": 2,
  "installCommand": "yarn install",
  "buildCommand": "nuxt build",
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "headers":[
    {
      "source": "/(.*-[V,C,R,D,B,N,M][0-9]*$)",
      "headers" : [
        {
          "key" : "x-robots-tag",
          "value" : "noindex"
        }
      ]
    },
    {
      "source": "/(.*-[V,C,R,D,B,N,M][0-9]*$[V,C,R,D,B,N,M][0-9]*$)",
      "headers" : [
        {
          "key" : "x-robots-tag",
          "value" : "noindex"
        }
      ]
    }
  ]
}
