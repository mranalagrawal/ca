{
  "version": 3,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["package.json"]
      }
    }
  ],
  "routes": [
    {
      "source": "/(.*)",
      "continue": true,
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
